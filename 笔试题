char* GetMemory(char* p)
{
	p = (char*)malloc(100);
	return p;
}
void Test(void)
{
	char* str = NULL;
	str = GetMemory(str);
	strcpy(str,"hello world");
	printf(str);
	free(str);
}
int main()
{
	Test();
	return 0;
}
